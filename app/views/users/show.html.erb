<h1>Hello, <%= @user.username %>.</h1>

<%= button_to("Log Out", destroy_user_session_path, method: :delete) %>

<% if @friend_requests.any? %>
  <h2>Friend Requests</h2>
  <% @friend_requests.each do |friend_request| %>
    <p>
      From: <%= friend_request.sender.username %>
    </p>
  <% end %>
<% end %>
